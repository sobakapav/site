import '~/components/main/Bento.less';

const metadata = {
  canonical: 'https://sobakapav.ru/services',
  title: `UX-аутстафф • Возьмем на себя все ваши дизайнерские задачи`,
  description:
    'Внешний отдел UX-специалистов: от продуктовых исследований до интерфейсов в html/css.',
  robots: {
    index: true,
    follow: true,
  },
  openGraph: {
    site_name: 'Собака Павлова',
    images: [
      {
        url: '~/assets/images/services.png',
        width: 1200,
        height: 630,
      },
    ],
    type: 'website',
