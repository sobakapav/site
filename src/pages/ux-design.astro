---
import Layout from '~/layouts/PageLayout.astro';

import Hund2 from '~/components/widgets/Hund2.astro';
import Hund3 from '~/components/widgets/Hund3.astro';
import Hund4 from '~/components/widgets/Hund4.astro';
import Bread from '~/components/widgets/Bread.astro';
import Image from '~/components/common/Image.astro';
import TabComponent from '~/components/main/TabComponent.astro';
import LogoTabComponent2 from '~/components/main/LogoTabComponent2.astro';
import RelatedPages from '~/components/portfolio/RelatedPages.astro';
import RelatedLinks from '~/components/services/RelatedLinks.astro';
import HighlightedCases from "~/components/services/HighlightedCases.astro";
import Number from '~/components/portfolio/Number.astro';
import ReviewItem from '~/components/portfolio/ReviewItem.astro';
import UniversalSlider from '~/components/common/UniversalSlider.astro'
import Features3 from '~/components/widgets/Features3.astro';
import FeaturesHex from '~/components/widgets/FeaturesHex.astro';
import SimpleSlider from '~/components/services/SimpleSlider.astro';

import { Icon } from 'astro-icon/components';
import { SITE } from 'astrowind:config';

import { getPermalink } from '~/utils/permalinks';
import { getEntry } from 'astro:content';

import '~/components/main/Bento.less';

const metadata = {
  canonical: 'https://sobakapav.ru/ux-design',
  title: `UX-дизайн • Дизайн и редизайн интерфейсов`,
  description:
    'Создаем интерфейсы сложных систем. Рабочие места, дашборды, мобильные приложения, системы мониторинга, личные кабинеты, интерфейсы для hardware. Под ключ или точечно, наброски или тщательная проработка, независимо или внутри вашей команды.',
  robots: {
    index: true,
    follow: true,
  },
  openGraph: {
    site_name: 'Собака Павлова',
    images: [
      {
        url: '~/assets/images/services/turnkey/ux-design.png',
        width: 716,
        height: 600,
      },
    ],
    type: 'website',
  },
};
---

<Layout metadata={metadata}  hasSlider={true}>
  <Bread title="UX-дизайн" />
  
  <Hund2
    image={{
      src: '~/assets/images/services/turnkey/ux-design.svg',
      alt: 'UX-дизайн • Собака Павлова',
    }}
    imageSafari={{
      src: '~/assets/images/services/turnkey/ux-design.png',
      alt: 'UX-дизайн • Собака Павлова',
    }}
    imageName="UX-аутстафф • Собака Павлова"
  >
    
    <Fragment slot="title">
      Дизайн интерфейсов
    </Fragment>
    
    <Fragment slot="subtitle">
      Сделаем ваш IT-продукт удобным и приятным в использовании.
    </Fragment>
    
  </Hund2>
  
  <section class="py-0 mx-auto grow-0 shrink-0">
     <article>

  <div class="flex flex-row flex-wrap text-base gap-2.5 mt-10 mb-10 px-4 md:px-6 mx-auto max-w-7xl text-[#9F9F9F]">
    <span>
      <a href="#1" class="underline underline-offset-2">Качественно</a>
    </span><span> • </span>
    
    <span>
      <a href="#2" class="underline underline-offset-2">Красиво</a>
    </span><span> • </span>
    
    <span>
      <a href="#3" class="underline underline-offset-2">Технологично</a>
    </span><span> • </span>
    
    <span>
      <a href="#4" class="underline underline-offset-2">Осмысленно</a>
    </span><span> • </span>
    
    <span>
      <a href="#5" class="underline underline-offset-2">В удобном вам формате</a>
      
  </div>
          
  <a id="1"></a>
  
  <div class=" flex flex-col max-w-7xl mx-auto px-4 sm:px-6 mb-10 md:mb-20">
    <h2 class="text-xl font-heading tracking-widest leading-none text-[--aw-color-hund] mb-4">Качественно</h2>
    
    <FeaturesHex
      title="Обновляем переделываем улучшаем"
      items={[{title: 'Дизайн продуктов', description: 'Разбираемся в сути продукта, а не только в кнопках. Делаем дизайн, который помогает, а не мешает'}, {title: 'Редизайн приложений и сайтов', description: 'Аккуратно улучшаем, где можно, и радикально меняем, где нужно. Чтобы стало удобно, понятно и не стыдно'}, {title: 'UX/UI-дизайн', description: 'Сначала думаем, потом рисуем. UX и UI — в нужных пропрциях и на своём месте.'}, {title: 'Обновление старого дизайна', description: 'Бережно обновим то, что устарело. Без революций — только нужные улучшения.'}, {title: 'Переработка мобильных приложений', description: 'Перепроектируем, перерисуем, улучшим. Чтобы работало лучше, выглядело свежее и не теряло пользователей.'}]}
      classes = {{items: {description: 'text-xl mt-1'}}}
    />
  </div>
  
  <a id="2"></a>
    
  <div class=" flex flex-col max-w-7xl mx-auto px-4 sm:px-6">
      <h2 class="text-xl font-heading tracking-widest leading-none text-[--aw-color-hund] mb-4">Красиво</h2>
  </div>
    
  <SimpleSlider 
    mode="mode-2"
    title=""
    slides={[{title: 'Создаём для вас фирменный стиль или уважаем существующий', img: '/images/services/landing/1.png', alt: 'UX-дизайн • Собака Павлова'},
