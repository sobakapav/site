---
import type { InferGetStaticPropsType, GetStaticPaths } from 'astro';

import Layout from '~/layouts/PageLayout.astro';
import Hero from '~/components/widgets/Hero.astro';
import PortfolioCollection from '~/components/portfolio/PortfolioCollection.astro';
import Headline from '~/components/blog/Headline.astro';
import { getEntry } from 'astro:content';

const { page } = Astro.props as Props;

const metadata = {
  title: 'Портфолио',
};

---

<Layout metadata={metadata}>
  <section class="px-6 sm:px-6 py-12 sm:py-16 lg:py-20 mx-auto max-w-6xl">
    <!-- Hero Widget ******************* -->
    
      <Hero
        tagline=""
        actions={[
          {
            variant: 'primary',
            target: '_blank',
            text: 'Get templates',
            href: 'https://github.com/onwidget/astrowind',
            icon: 'tabler:download',
          },
          { text: 'Learn more', href: '#features' },
        ]}
      >
        <Fragment slot="title">
          Портфолио <br/><span class="text-accent dark:text-white highlight">«Собаки Павловой»</span> 
        </Fragment>
    
        <Fragment slot="subtitle">
            Подробные описания проектов, демонстрация итогов и промежуточных результатов, цены и отзывы клиентов. Читайте, пожалуйста ↓↓↓   
        </Fragment>
    
      </Hero>
    <PortfolioCollection list={['tmh', 'dipol', 'prozhito', 'control-patent','e-gorod-mobile', 'alfastrah-design', 'itmo', 'onkor', 'avicenna',  'zapovednik', 'compel', 'qiwi', 'control-predpisanie', 'stroynastroy',  'domotech', 'e-gorod-mobile', 'pangeo', 'ot-vinta', 'alfastrah-design', 'anggell', 'medmarket',  'data-solution', 'res', 'tis', 'ohlebe', 'm-line', 'neurobureau', 'monetory', 'math', 'elma', 'easydocs', 'brighteye', 'raisa', 'kapital', 'alfa-arm', 'gazprom-neft', 'okey', 'dau', 'algohelp', 'e-gorod-guide', 'novotelekom', 'petrovich', 'video-santa', 'fasting', 'avito', 'control-mobile', 'control', 'gormost', 'trash', 'boracase', 'veza', 'fishlook', 'cindicator', 'documedd', 'miaz', 'alfastrahhr', 'edms', 'stream-telecom', 'sindbad', 'e-gorod', 'atol', 'smartdeal', 'geonaft', 'costa', 'nlmk', 'sberbank', 'electronika', 'crt-panel-administratora', 'center-rechevyh-technologiy', 'faberlic', 'amber', 'serverscom', 'vuborgskiy-zamok', 'tigitl','relief-center', 'alfa', 'tinkoff', 'sportmaster', 'relefopt', 'autotransinfo', 'tiomed', 'f-kabinki', 'yit', 'viking-line', 'alfabank']} />
